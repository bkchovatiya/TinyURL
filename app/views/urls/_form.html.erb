<%= form_with(model: url) do |form| %>
  <% if url.errors.any? %>
    <div style="color: red">
      <h2><%= pluralize(url.errors.count, "error") %> prohibited this url from being saved:</h2>

      <ul>
        <% url.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>
  <table class="table">
      
    <tbody>
      <tr>
        <td style="border:none">
          <%= form.label :original, style: "display: block" %>
          <%= form.text_field :original %>
        </td>
      </tr>
      <tr style="border:none">
        <td style="border:none"><%= form.submit %></td>
      </tr>
    </tbody>
  </table>
 <% end %>